<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="377.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.controllers.ChooseWishListController">
   <children>
      <JFXButton fx:id="btnBack" layoutX="14.0" layoutY="14.0" onAction="#back" prefHeight="57.0" prefWidth="60.0" style="-fx-background-color: #d9d9d9;" text="&lt;-">
         <font>
            <Font size="24.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="btnAdd" layoutX="307.0" layoutY="14.0" onAction="#addWishList" prefHeight="57.0" prefWidth="60.0" style="-fx-background-color: #c9e265;" text="+">
         <font>
            <Font size="24.0" />
         </font>
      </JFXButton>
      <JFXTextField fx:id="textFieldSearch" alignment="CENTER" layoutX="88.0" layoutY="24.0" onKeyReleased="#search" prefHeight="26.0" prefWidth="205.0" promptText="Wishlist name">
         <font>
            <Font size="16.0" />
         </font>
      </JFXTextField>
      <ScrollPane layoutY="82.0" prefHeight="318.0" prefWidth="377.0">
         <content>
            <VBox fx:id="list" alignment="CENTER" prefHeight="313.0" prefWidth="372.0" />
         </content>
      </ScrollPane>
   </children>
</AnchorPane>
